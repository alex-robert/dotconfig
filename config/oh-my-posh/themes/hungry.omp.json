{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 3,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "\u256d\u2500\ue0b2",
          "background": "#6CA35E",
          "foreground": "#272727",
          "properties": {
            "macos": "\uea85"
          },
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background": "#272727",
          "foreground": "#a6a4a6",
          "properties": {
            "style": "agnoster_short",
            "home_icon": "\udb81\udf25",
            "max_depth": 2,
            "folder_icon": "\u2026",
            "folder_separator_icon": " <transparent>\ue0b1</> "
          },
          "template": " {{ .Path }} "
        },
        {
          "type": "project",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background": "#83769c",
          "foreground": "#011627",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}\ueb29 <transparent>\ue0b1</>{{ if .Name }} {{ .Name }} {{ end }}{{ if .Version }} v{{.Version}} {{ end }}{{ end }} "

        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#272727",
          "background": "#6CA35E",
          "template": " \ueb0f <transparent>\ue0b1</> {{ .Full }} "
        },


        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background": "#191f48",
          "foreground": "#43CCEA",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": false,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "branch_icon": "\uf418 ",
            "branch_identical_icon": "\uf444",
            "branch_gone_icon": "\ueab8"
          },
          "template": " {{ .UpstreamIcon }} {{ .HEAD }} {{if .BranchStatus }} {{ .BranchStatus }}{{ end }} ",
          "trailing_diamond": "\ue0b0"
        },


        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "background": "#2f2f2f",
          "foreground": "#ffff66",
          "properties": {
            "style": "roundrock",
            "threshold": 500,
            "always_enabled": false
          },
          "template": " \uf252 {{ .FormattedMs }} "
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#6CA35E",
          "template": "\u2570\u2500{{ if .Root }}#{{else}}\ue602{{end}}"
        }
      ],
      "type": "prompt"
    }
  ]
}
